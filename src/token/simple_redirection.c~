#include "../../inc/minishell2.h"

t_exe	*left_redir(t_exe *do_do, t_env **l_env, t_put *curr)
{
  r_in = open(to_do->left, O_RONLY);
  return (to_do);
}

t_exe	*right_redir(t_exe *do_do, t_env **l_env, t_put *curr)
{
  r_out = open(to_do->left, O_WRONLY);
  return (to_do);
}
