#include "../../inc/minishell2.h"

int	pipe_shell(char **argv, t_exe *to_do, t_env **l_env, int pid_f)
{
  printf("Dans pipe va executer :");
  my_show_tab(argv);
  exec(argv, l_env);
  allocate_process(to_do->right, l_env);
}
