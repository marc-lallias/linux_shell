#include "../../inc/minishell2.h"

int	choose_token(t_exe *to_do)
{
  if ((match_n_match(*(to_do->data_tab), "pipe")) == 1)
    {
      printf("La\n");
      pipe_shell(to_do);
    }
  return (exec(to_do->data_tab));
}
